<%include ../../header_index.ejs %>

<!-- Side Menu -->
<%include sidemenu %>
<!-- /Side Menu -->
<blockquote><h2>Welcome to <%= title %></h2></blockquote>

<form class="form-horizontal" role="form" action="/user/add" method="get">

    <div class="form-group">
        <label for="inputEmail3" class="col-sm-1 control-label">Email</label>
        <div class="col-sm-3">
            <input class="form-control" type="text" name="email"> <br>
        </div>
    </div>
    <div class="form-group">
        <label for="inputPassword3" class="col-sm-1 control-label">First Name</label>
        <div class="col-sm-3">
            <input class="form-control" name="fname" type="text"> <br>
        </div>
    </div>
    <div class="form-group">
        <label for="inputPassword3" class="col-sm-1 control-label">Last Name</label>
        <div class="col-sm-3">
            <input class="form-control"  name="lname" type="text"> <br>
        </div>
    </div>
    <div class="form-group">
        <label for="inputPassword3" class="col-sm-1 control-label">Password</label>
        <div class="col-sm-3">
            <input class="form-control" type="password" name="password"> <br>
        </div>
    </div>
    <div class="form-group">
        <div class="col-sm-offset-2 col-sm-10">
            <input type="submit" value="Add User"/>
        </div>
    </div>
</form>

<%include ../../footer_index.ejs %>